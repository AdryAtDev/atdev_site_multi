{% macro breadcrumb(page_title, service, home_link, services_link, about, aboutLink) %}
	{# BEGIN MATERIAL ICONS - HOME #}
	<link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@48,500,0,200"/>
	{# END MATERIAL ICONS - HOME #}

	<div id="breadcrumbInnerContainer" class="container">
		<div id="breadcrumb">
			{% if service != undefined %}
				<p>
					<a href="{{ home_link }}" style="padding-bottom: 1.6rem !important;">
						<img src="/icons/home.svg" alt="" style="height: 1rem; margin-top: -0.2rem"/>
					</a>
					<span>></span>
					<a href="{{ services_link }}" style="padding-bottom: 1.6rem !important;">{{ service | capitalize }}</a>
					<span>></span>
					<b style="font-family: GothamBold,  Lato, 'GothamBook', 'Plus Jakarta Sans', sans-serif;">{{ page_title | capitalize }}</b>
				</p>
			{% elif about != undefined  %}
				<p>
					<a href="{{ home_link }}" style="padding-bottom: 1.6rem !important;">
						<img src="/icons/home.svg" alt="" style="height: 1rem; margin-top: -0.2rem"/>
					</a>
					<span>></span>
					<a href="{{ aboutLink }}" style="padding-bottom: 1.6rem !important;">{{ about | capitalize }}</a>
					<span>></span>
					<b style="font-family: GothamBold,  Lato, 'GothamBook', 'Plus Jakarta Sans', sans-serif;">{{ page_title | capitalize }}</b>
				</p>
			{% else %}
				<p>
					<a href="{{ home_link }}" style="padding-bottom: 1.6rem !important;">
						<img src="/icons/home.svg" alt="img" style="height: 1rem; margin-top: -0.2rem"/>
					</a>
					<span>></span>
					<b style="font-family: GothamBold,  Lato, 'GothamBook', 'Plus Jakarta Sans', sans-serif;">{{ page_title | capitalize }}</b>
				</p>
			{% endif %}
		</div>
	</div>
{% endmacro %}